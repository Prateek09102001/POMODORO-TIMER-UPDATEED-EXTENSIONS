{
    "manifest_version": 3,
    "name": "Pomodoro Timer",
    "version": "1.0",
    "decription":
    "permissions": [
        "storage",
        "notifications",
        "alarms"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icons/logo-16.png",
            "48": "icons/logo-48.png",
            "128": "icons/logo-128.png"
        }
    },
    "icons": {
        "16": "icons/logo-16.png",
        "48": "icons/logo-48.png",
        "128": "icons/logo-128.png"
    }
}
