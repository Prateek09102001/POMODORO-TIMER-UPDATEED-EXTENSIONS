{
    "manifest_version": 3,
    "name": "Pomodoro Timer",
    "version": "1.0",
    "permissions": ["notifications", "contextMenus", "alarms"],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": "icons/logo-32.png"
    },
    "icons": {
        "32": "icons/logo-32.png",
        "16": "icons/logo-32.png",
        "64": "icons/logo-.png",
        "128": "icons/logo-128.png"
    }
}
